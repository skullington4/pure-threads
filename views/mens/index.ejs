<%- include('../partials/header') %>

<h2>Men's department</h2>


<table id="list">
    <thead>
      <tr>
        <th></th>
        <th>Title:</th>
      </tr>
    </thead>
    <tbody>
      <% items.forEach(function(i) { %>
        <tr>
          <td><a href="/mens/<%= i._id %>" class="indexPictures"><img src="<%= i.imageOne %>"></a></td>
          <td><%= i.title %></td>
        </tr>
        <% }); %>
    </tbody>
</table>


<%- include('../partials/footer') %>