<%- include('../partials/header') %>

<h2>Welcome to the women's department</h2>

<table id="list">
  <thead>
    <tr>
      <th></th>
      <th>Title:</th>
    </tr>
  </thead>
  <tbody>
    <% items.forEach(function(i) { %>
      <tr>
        <td><a href="/womens/<%= i._id %>" class="indexPictures"><img src="<%= i.imageOne %>"></a></td>
        <td><a href="/womens/<%= i._id %>"><%= i.title %></a></td>
      </tr>
      <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>